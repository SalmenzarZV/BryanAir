<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BillBuyActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/buyFragment"
        android:name="com.pmdm.almenzarjimenezsergio.bryanair.BuyFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/billFragment"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/buy_activity" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/billFragment"
        android:name="com.pmdm.almenzarjimenezsergio.bryanair.BillFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buyFragment"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/activity_bill" />
</androidx.constraintlayout.widget.ConstraintLayout>